    "@tanstack/react-query": "^5.8.4",
    "@types/uuid": "^10.0.0",
    "axios": "^1.6.2",
    "dayjs": "^1.11.10",
    "firebase": "^11.2.0",
    "moment-timezone": "^0.5.47",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.5.1",
    "react-router-dom": "^6.20.0",
    "uuid": "^11.0.5",
    "zustand": "^4.4.6"
