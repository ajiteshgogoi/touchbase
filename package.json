{
  "name": "touchbase",
  "type": "module",
  "private": true,
  "version": "2.5.7",
  "scripts": {
    "predev": "node scripts/update-version.js && npm run build:scripts && node dist/scripts/generate-blog-static.js && node scripts/import-blog-to-sanity.js",
    "dev": "vite",
    "prebuild": "node scripts/update-version.js && node scripts/generate-sw-config.js && node scripts/generate-sitemap.js && node scripts/generate-feeds.js && npm run build:scripts && node dist/scripts/generate-blog-static.js && node scripts/import-blog-to-sanity.js",
    "build": "tsc && vite build",
    "build:scripts": "tsc -p tsconfig.scripts.json",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "clean-caches": "node scripts/clean-caches.js",
    "bump-version": "node scripts/bump-version.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "supabase:types": "supabase gen types typescript --local > src/lib/supabase/database.types.ts",
    "daily-check": "node dist/services/daily-check.js",
    "sync-users-to-brevo": "node scripts/sync-existing-users-to-brevo.js",
    "generate:blog": "npm run build:scripts && node dist/scripts/generate-blog-static.js",
    "import:blog": "node scripts/import-blog-to-sanity.js",
    "generate:feeds": "node scripts/generate-feeds.js"
  },
  "dependencies": {
    "@babel/runtime": ">=7.26.10",
    "@floating-ui/react": "^0.27.6",
    "@headlessui/react": "^2.2.0",
    "@heroicons/react": "^2.0.18",
    "@paypal/react-paypal-js": "^8.8.2",
    "@portabletext/block-tools": "^1.1.16",
    "@portabletext/react": "^3.2.1",
    "@sanity/client": "^6.14.0",
    "@sanity/image-url": "^1.1.0",
    "@sanity/types": "^3.0.0",
    "@sanity/vision": "^3.86.1",
    "@supabase/supabase-js": "^2.39.0",
    "@tailwindcss/postcss": "^4.0.3",
    "@tanstack/react-query": "^5.8.4",
    "@types/react-window": "^1.8.8",
    "@types/uuid": "^10.0.0",
    "axios": ">=1.8.2",
    "cookie": ">=0.7.0",
    "dayjs": "^1.11.10",
    "esbuild": "^0.25.0",
    "firebase": "^11.2.0",
    "gray-matter": "^4.0.3",
    "marked": "^15.0.7",
    "react": "^18.2.0",
    "react-confetti": "^6.4.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.5",
    "react-hot-toast": "^2.5.1",
    "react-intersection-observer": "^9.16.0",
    "react-router-dom": "^6.20.0",
    "react-window": "^1.8.11",
    "sanity": "^3.86.1",
    "uuid": "^11.0.5",
    "zustand": "^4.5.6"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.10",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/node": "^20.9.4",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.1",
    "@types/react-is": "^19.0.0",
    "@types/use-sync-external-store": "^1.5.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "@vitest/coverage-v8": "^0.34.6",
    "@vitest/ui": "^0.34.6",
    "autoprefixer": "^10.4.16",
    "chalk": "^5.4.1",
    "dotenv": "^16.4.7",
    "eslint": "^8.54.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "jsdom": "^22.1.0",
    "node-fetch": "^3.3.2",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.3.2",
    "vite": "^5.0.0",
    "vite-plugin-pwa": "^0.17.5",
    "vitest": "^0.34.6",
    "web-push": "^3.6.7",
    "wrangler": "^4.3.0"
  }
}